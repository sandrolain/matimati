{
  "name": "mati-mati",
  "version": "0.1.3",
  "description": "Learning app for my daughter",
  "author": "Sandro Lain",
  "main": "dist/umd/index.js",
  "browser": "dist/umd/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/esm/index.d.ts",
  "files": [
    "dist/esm/*",
    "dist/umd/*"
  ],
  "scripts": {
    "build": "npx rollup -c",
    "watch": "npx rollup -cw",
    "prepare": "npm run build",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:coverage:open": "jest --coverage && open ./coverage/lcov-report/index.html",
    "docs:build": "npx typedoc ./src",
    "docs:serve": "npm run docs:build && npx open-cli ./docs/typedocs/index.html"
  },
  "keywords": [
    "mati",
    "game",
    "learning"
  ],
  "license": "MIT",
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^7.1.3",
    "@types/jest": "^25.2.1",
    "@types/node": "^13.13.0",
    "@typescript-eslint/eslint-plugin": "^2.28.0",
    "@typescript-eslint/parser": "^2.28.0",
    "eslint": "^6.8.0",
    "happy-dom": "^0.12.1",
    "jest": "^25.3.0",
    "open-cli": "^6.0.1",
    "rollup": "^2.6.1",
    "rollup-plugin-delete": "^1.2.0",
    "rollup-plugin-terser": "^5.3.0",
    "rollup-plugin-typescript2": "^0.27.0",
    "ts-jest": "^25.4.0",
    "typedoc": "^0.17.4",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "htna": "git+https://github.com/sandrolain/HTNA.git#develop",
    "htna-components": "git+https://github.com/sandrolain/HTNA-components.git#develop",
    "htna-tools": "git+https://github.com/sandrolain/HTNA-tools.git#develop"
  }
}
